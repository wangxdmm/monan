{
  "name": "@monan/shared",
  "type": "module",
  "version": "1.0.6",
  "exports": {
    ".": {
      "types": {
        "development": "./src/index.ts",
        "default": "./types/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "pnpm exec unbuild --watch",
    "build": "pnpm dts && pnpm exec unbuild",
    "dts": "pnpm exec cdt"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "fast-copy": "^3.0.2"
  },
  "devDependencies": {
    "@monan/types": "workspace:^",
    "@types/clone": "^2.1.4",
    "@types/throttle-debounce": "^5.0.2",
    "throttle-debounce": "^5.0.2"
  }
}
