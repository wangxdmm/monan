{
  "name": "@monan/types",
  "type": "module",
  "version": "2.0.0",
  "exports": {
    ".": {
      "types": {
        "development": "./src/index.ts",
        "default": "./types/index.d.ts"
      }
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./*": "./*"
  },
  "types": "types/index.d.ts",
  "files": [
    "README.md",
    "package.json",
    "types"
  ],
  "scripts": {
    "dev": "pnpm exec unbuild --watch",
    "build": "pnpm dts",
    "dts": "pnpm exec cdt -p node"
  },
  "publishConfig": {
    "access": "public"
  }
}
