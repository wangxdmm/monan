{
  "name": "tree-shaking-test",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": {
        "development": "./src/index.ts",
        "default": "./types/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "pnpm exec unbuild --watch",
    "build": "pnpm dts && pnpm exec unbuild",
    "dts": "pnpm exec cdt"
  },
  "devDependencies": {
    "@monan/shared": "workspace:^",
    "@monan/types": "workspace:^"
  }
}
