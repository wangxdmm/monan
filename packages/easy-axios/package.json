{
  "name": "@monan/easy-axios",
  "type": "module",
  "version": "2.0.2",
  "exports": {
    ".": {
      "types": {
        "development": "./src/index.ts",
        "default": "./types/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json",
    "types"
  ],
  "scripts": {
    "dev": "pnpm exec unbuild --watch",
    "build": "pnpm dts && pnpm exec unbuild",
    "dts": "pnpm exec cdt -p pure"
  },
  "peerDependencies": {
    "axios": ">= 1.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@monan/shared": "workspace:*",
    "@monan/types": "workspace:*",
    "ohash": "^1.1.3"
  }
}
